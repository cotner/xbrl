<?xml version="1.0" encoding="UTF-8"?>
<link:linkbase
  xmlns:link="http://www.xbrl.org/2003/linkbase"
  xmlns:generic="http://xbrl.org/2008/generic"
  xmlns:label="http://xbrl.org/2008/label"
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
  http://www.xbrl.org/2003/linkbase http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd
  http://xbrl.org/2008/generic http://www.xbrl.org/2008/generic-link.xsd
  http://xbrl.org/2008/label http://www.xbrl.org/2008/generic-label.xsd
  ">

  <link:arcroleRef 
  xlink:type="simple" 
  arcroleURI="http://xbrl.org/arcrole/2008/element-label" 
  xlink:href="http://www.xbrl.org/2008/generic-label.xsd#element-label"/>
  
  <generic:link xlink:type="extended" xlink:role="http://www.xbrl.org/2003/role/link">
    <link:loc xlink:href="commentLabelRole.xsd#comment" xlink:type="locator" xlink:label="role"/>
    <label:label xlink:type="resource" xlink:label="label" 
    xlink:role="http://www.xbrl.org/2003/role/label" xml:lang="en">
      comment
    </label:label>    
    <generic:arc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/element-label"
      xlink:from="role" xlink:to="label" order="1.0"/>
  </generic:link>

</link:linkbase>
